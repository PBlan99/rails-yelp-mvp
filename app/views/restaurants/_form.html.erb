<%= form_for(@restaurant) do |f| %>
  <%= f.label :name,'Restaurant name:' %>
  <%= f.text_field :name %>

  <%= f.label :address,'Restaurant address:' %>
  <%= f.text_field :address %>

  <%= f.label :phone_number,'Restaurant phone number:' %>
  <%= f.text_field :phone_number %>

  <%= f.label :category,'Restaurant category:' %>
  <%= f.select :category,['chinese', 'italian', 'japanese', 'french', 'belgian'] %>

  <%= f.submit %>
<% end %>
